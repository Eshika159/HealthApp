<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adity.healthcareapp2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DiseaseActivity" />
        <activity android:name=".HealthCareHome" />
        <activity android:name=".SignUp" />
        <activity android:name=".UserProfileActivity" />
        <activity
            android:name=".NavActivity"
            android:label="@string/title_activity_nav"
            android:theme="@style/AppTheme" />
        <activity android:name=".SearchDoctorActivity" />
        <activity
            android:name=".Chat"
            android:label="Discussion Forum"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".SuggestActivity"></activity>
    </application>

</manifest>